public class Main {
    public static void main(String[] args) {
        getAdd(10, 20);
        addRubbish("Hello , I am ");
        addName("Sultan");
        addSurname("Zhanyshev");
        addRub("I am ");
        addAge(20);
        addRubb("Goodbye!");

    }

    public static void getAdd(int age, int temperature) {
        System.out.println(age + " лет  " + " градусов " + temperature);
        if (20 <= age && age <= 45 && temperature >= -20 && temperature <= 30) {
            System.out.println("Можно идти гулять");
        } else if (age <= 20 && temperature <= 28 && temperature >= 0) {
            System.out.println("Можно идти гулять");
        } else if (age >= 45 && temperature <= -10 && temperature >= 25) {
            System.out.println("Можно идти гулять");

        } else {
            System.out.println("Оставайтесь дома");
        }

    }

    public static void addRubbish(String rubbish) {
        System.out.println(rubbish);
    }


    public static void addName(String name) {
        System.out.println(name);
    }

    public static void addSurname(String surName) {
        System.out.println(surName);
    }

    public static void addRub(String rub) {
        System.out.println(rub);
    }

    public static void addAge(int myAge) {
        System.out.println(myAge);
    }


    public static void addRubb(String rubb) {
        System.out.println(rubb);
    }

}
